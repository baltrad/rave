JDK_ROOT=
JDK_SUPRESSED=yes
JDK_VERSION=
case $withval in
  no)
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for java" >&5
printf %s "checking for java... " >&6; }
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: suppressed" >&5
printf "%s\n" "suppressed" >&6; }
    ;;
  *)
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for java" >&5
printf %s "checking for java... " >&6; }
    java_jdk_root="`echo $withval`"
    if test "yes" = "$java_jdk_root"; then
      java_bin_dir=`which java | sed -e "s/\/java$//g"`;
      javac_bin_dir=`which javac | sed -e "s/\/javac$//g"`;
      jdk_root=`echo $java_bin_dir | sed -e "s/\/bin$//g"`
    else
      java_bin_dir="$java_jdk_root/bin";
      javac_bin_dir="$java_jdk_root/bin";
      jdk_root="$java_jdk_root"
    fi
    if [ "$java_bin_dir" != "$javac_bin_dir" ]; then
      as_fn_error $? "java and javac bin directories seems to be placed at different places" "$LINENO" 5
    elif [ "$java_bin_dir" = "" ]; then
      as_fn_error $? "Java: No jdk-home dir reckognized" "$LINENO" 5
    fi
    javaversion=`$java_bin_dir/java -version 2>&1 | grep "java version \"" | sed -e "s/\(java version \"\)\([0-9\.]*\)\(_.*\"\)/\2/g"`
    if [ $? -ne 0 ]; then
      as_fn_error is this OpenJDK or SUN JDK? "Failure to execute $java_bin_dir/java -version" "$LINENO" 5
    fi
    mx_jversion=`echo $javaversion | cut -d '.' -f1`
    ma_jversion=`echo $javaversion | cut -d '.' -f2`
    if [ "$mx_jversion" = "" -o "$ma_jversion" = "" ]; then
      as_fn_error found $javaversion "Java must be OpenJDK or Sun JDK version 1.6 or higher" "$LINENO" 5
    fi
    if [ $mx_jversion -ne 1 -o $ma_jversion -lt 6 ]; then
      as_fn_error found $javaversion "Java must be OpenJDK or SUN version 1.6 or higher" "$LINENO" 5
    else
      JDK_ROOT="$jdk_root"
      JDK_SUPRESSED=no
      JDK_VERSION=$javaversion
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $JDK_ROOT" >&5
printf "%s\n" "$JDK_ROOT" >&6; }
    fi
    ;;
esac

