  case $ac_config_target in
    "def.mk") CONFIG_FILES="$CONFIG_FILES def.mk" ;;
    "librave/toolbox/rave_config.h") CONFIG_FILES="$CONFIG_FILES librave/toolbox/rave_config.h" ;;

