# This is a utility for beeing able to extract the include/lib from the --with directive.
extract_include_or_lib_from_with() {
  if [ "`echo $2 | grep ','`" = "" ]; then
    proj_inc=$2/include
    proj_lib=$2/lib
  else
    proj_inc="`echo $2 |cut -f1 -d,`"
    proj_lib="`echo $2 |cut -f2 -d,`"
  fi
  if [ "$1" = "include" ]; then
    echo "$proj_inc"
  else
    echo "$proj_lib"
  fi
}

